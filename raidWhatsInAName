def calculation2num(x, y):
    z = x * y
    return z

def calculation3num(a, b, c):
    temp = calculation2num(a, b)
    result = temp * c
    return result

length = 5
width = 3
plane = calculation2num(length, width)
print(f"The rectangles's size is: {plane}")

length = 4
width = 6
height = 2
cube = calculation3num(length, width, height)
print("The cube's size is: ", cube)